{
  "name": "Media Keys for Spotify ‚èØ",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Allows you to use the native Spotify media keys on Spotify",
  "icons": {
    "16": "images/logo-small.png",
    "128": "images/logo.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://open.spotify.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://open.spotify.com/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
